{
  "name": "@lyyti/design-system",
  "description": "Lyyti Design System",
  "homepage": "https://lyytioy.github.io/lyyti-design-system",
  "version": "3.3.0",
  "engines": {
    "node": "^18",
    "npm": "^9"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc",
    "test-storybook": "test-storybook",
    "build-storybook": "npm ci && storybook build -o docs-build",
    "build": "rm -rf dist/* && tsc --project tsconfig.release.json && cp -r src/fonts dist && cp -r src/styles dist && cp package.json dist && cp README.md dist",
    "bump": "npm --no-git-tag-version version",
    "generate-icon": "npx @svgr/cli --typescript --template templateSvgIcon.ts -d src/icons --ext tsx $npm_config_path"
  },
  "resolutions": {
    "**/@emotion/styled": "^11.0.0"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/lab": "5.0.0-alpha.134",
    "@mui/material": "5.14.10",
    "@mui/x-data-grid": "6.13.0",
    "@mui/x-data-grid-pro": "6.7.0",
    "@mui/x-date-pickers": "6.4.0",
    "@mui/x-license-pro": "6.10.2"
  },
  "devDependencies": {
    "@babel/core": "7.22.20",
    "@date-io/dayjs": "2.16.0",
    "@date-io/moment": "2.17.0",
    "@storybook/addon-actions": "7.5.1",
    "@storybook/addon-essentials": "7.0.24",
    "@storybook/addon-links": "7.5.1",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/nextjs": "7.4.5",
    "@storybook/react": "7.4.6",
    "@storybook/test-runner": "0.10.0",
    "@svgr/cli": "8.1.0",
    "@types/node": "20.8.3",
    "@types/react": "18.2.11",
    "babel-loader": "9.1.3",
    "dayjs": "1.11.9",
    "eslint": "8.41.0",
    "eslint-config-next": "12.3.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-storybook": "0.6.12",
    "moment": "2.29.4",
    "next": "13.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.4.2",
    "typescript": "4.9.5",
    "webpack": "5.88.2"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "git@github.com:lyytioy/lyyti-design-system.git",
  "author": "Lyyti Oy <help@lyyti.com>",
  "license": "MIT"
}
